    JAWA['rê'] = 'ꦉ';
    JAWA['ubti'] = 'ꦹꦧ꧀ꦠꦶ';
    JAWA['ucti'] = 'ꦹꦕ꧀ꦠꦶ';
    JAWA['udti'] = 'ꦹꦢ꧀ꦠꦶ';
    JAWA['ugti'] = 'ꦹꦒ꧀ꦠꦶ';
    JAWA['ujti'] = 'ꦹꦗ꧀ꦠꦶ';
    JAWA['ukti'] = 'ꦹꦏ꧀ꦠꦶ';
    JAWA['ulti'] = 'ꦹꦭ꧀ꦠꦶ';
    JAWA['umti'] = 'ꦹꦩ꧀ꦠꦶ';
    JAWA['upti'] = 'ꦹꦥ꧀ꦠꦶ';
    JAWA['urti'] = 'ꦹꦫ꧀ꦠꦶ';
    JAWA['utti'] = 'ꦹꦠ꧀ꦠ';
    JAWA['uwti'] = 'ꦹꦮ꧀ꦠꦶ';
    JAWA['unti'] = 'ꦹꦤ꧀ꦠꦶ';
    JAWA['ungti'] = 'ꦹꦔ꧀ꦠꦶ';
    JAWA['bukti'] = 'ꦨꦹꦏ꧀ꦠꦶ';
    JAWA['bakti'] = 'ꦨꦏ꧀ꦠꦶ';
    JAWA['sakti'] = 'ꦯꦏ꧀ꦠꦶ'; 
      
    // angka
    JAWA['0'] = '꧐';
    JAWA['1'] = '꧑';
    JAWA['2'] = '꧒';
    JAWA['3'] = '꧓';
    JAWA['4'] = '꧔';
    JAWA['5'] = '꧕';
    JAWA['6'] = '꧖';
    JAWA['7'] = '꧗';
    JAWA['8'] = '꧘';
    JAWA['9'] = '꧙';
    JAWA['.'] = '\uA9C9';
    JAWA[','] = '\uA9C8'; 
    JAWA['\u0020'] = '';
    JAWA['\u200c'] = '\u200c';
    JAWA['\u0028'] = '\uA9CC';
    JAWA['\u0029'] = '\uA9CD';
    JAWA['\u007B'] = '꧁';
    JAWA['\u007C'] = '\uA9CB';
    JAWA['\u007D'] = '꧂';
    JAWA[' '] = '꧋';
////////////////////////////////////////////////////////////////////////////
    
PAT_V     ='1'
PAT_VK    ='2'
PAT_KV    ='3'
PAT_KVK   ='4'
PAT_KCV   ='5'
PAT_KCVK  ='6'
////////////////////////////////////////////////////////////////////////////
function jawaahir (huruf)
////////////////////////////////////////////////////////////////////////////
{
    var retval = ''
    if (huruf == 'ng' || huruf == 'nk' || huruf == ' basa' || huruf == ' yasmi' || huruf == 'r' || huruf == ' omahku' || huruf == ' omahmu' || huruf == 'h') {
        retval = JAWA['+' + huruf]
    } else {
        retval = JAWA[huruf] + JAWA['+ï']
    }
    
    return retval
} // end jawaahir (...)
////////////////////////////////////////////////////////////////////////////
function Latin2Jawa(iStr)
////////////////////////////////////////////////////////////////////////////
{    
    var jawaText = ''
    
    iStr = iStr.toLowerCase()
    var iLength = iStr.length
    var idx = 0
    var jump = 0
    var tStr = ''
    var oStr = ''
    var r
    var lkj
    var suku
    var polasuku = '0'
    
    // Pola V, VK, K, KV, KVK, KCV, KCVK:
    var KONS = 'ngnny|nk| basa|id|snh|s nh|aia|dhh|thh|dars|durs|dors|ders|dêrs|dèrs|dérs|dairs|daurs|deors|ras|rdh|rjh|rsh|rgh|rbh|rkh|rch|rph|rnh|rth|saur|sair|sur|sor|ser|sèr|sér|sêr|syr|sk|sr|sp|bs|ps|rs|rf|rv|rq|rh|rm|rl|rj|rp|rn|ry|rw|rc|rg|rk|rd|rt|ngbh|ngkh|ngdh|ngth|ngch|ngjh|nggh|ngph|ngsh|ny|ngk|ngb|ngc|ngd|ngg|ngh|ngn|ngl|ngs|ngm|ngp|ngn|ngt|ngw|ngj|ngy|ngf|ngv|hdh|hch|hbh|sch|hgh|hjh|hkh|hph|hth|nch|nc|dh|hw|hs|hp|ht|hk|hg|hc|hq|hb|dz|kh|ui|bh|ea|êa|th|gh|hd|hl|hm|hn|n c|ch|ks|sc|sh|ss|ph|jn|jh|sjh|sj|sn|sl|sw|[b-dfgj-mpqtv-z]|nny|njh|njn|nj|rḍh|rṭh|nṭh|rṭ|nṭ|nḍh|ngḍh|ngṭh|ngñy|ngḍ|ngṭ|ngṣ|ngç|hḍ|hṭ|nḍ|rḍ|hṣ|nṭ|nh|hṭ|hḍ|ḍh|ṭh|ḥ|ṭ|ḍ|ṣ|j|hḍh|hṭh|sṭh|sḍh|sḍ|sṭ|nnyy|ng|h|ç|hñ|ñy|ñ|s|r|n'
    var VOK  = "bukti|sakti|bakti|aia|ia i|ubti|ucti|udti|ugti|ujti|ukti|ulti|umti|unti|upti|urti|usti|utti|uwti|ao|ia o|ia u|ia è|ia é|ia e|ia ê| leo| lê| le|re|rê| ia|aui|ee|aua|auo|auè|aué|auê|aue|aio|aiu|aiè|aié|aiê|aie|uai|uao|uau|oai|oau|oao|ao|aè|aé|aê|ae|ua|oa|ue|uê|uè|ué|oe|oê|oè|oé|o o|o a|o i|o u|o ê|o e|o è|oé|au i|au a|au o|au è|au é|au ê|au e|ai a|ai o|ai u|ai è|ai é|ai ê|ai e| ao| au| aè| aé| ae aê| ai|\u000A|ui|uè|ué|uê|ue|êi|ei|èi|éi|iu|iè|ié|iê|ie|io|oo|oa|ou|oê|oe|oè|oé|ie|a i|èa|éa|è e|é e|è a|é a|è o|é o|è u|é u|è i|é i|è è|é é|e a|e o|e u|e è|e é|e i| i|a u|a o|a è|a é|a ê|a e| i| u| o| é| è| ê| e| a|ai|au|éi|ei|aa|a a|i i|u u|u i|oai|oau|oeo|oêo|oi|u o|u é|u è|u a|i a|i o|i u|i è|i é|i ê|i e|iau|iao|ieo|iêo|eo|êo|ua|èo|éo|ia|io|iu|ua|eo|ii|uu|[aeiouèêé]|å"
    //VOK  = 'ras|aia|ia i|ubti|ucti|udti|ugti|ujti|ukti|ulti|umti|unti|upti|urti|usti|utti|uwti|ao|ia o|ia u|ia è|ia é|ia e|ia ê|re|rê|lê|le| aui|aua|auo|auè|aué|auê|aue|aio|aiu|aiè|aié|aiê|aie|o o|o a|o i|o u|o ê|o e|oè|o é|ai a|au i|au a|au o|au è|au é|au ê|au e|ai a|ai o|ai u|ai è|ai é|ai ê|ai e| ao| au| aè| aé| ae aê| ai|aè|aé|ae|aê|ui|uè|ué|uê|ue|êi|ei|èi|éi|iu|iè|ié|iê|ie|io|oo|oa|ou|oê|oè|oé|ie|a i|èa|éa|è e|é e|è a|é a|è o|é o|è u|é u|è i|é i|e a|e i|e u|e o|e è|e é|e i| i| u| o| é| è| ê| e| a|ai|au|éi|ei|aa|a a|i i|u u|u i|u o|u é|u è|u a|i a|i o|i u|i è|i é|i e|i ê|éo|êo|oai|oau|oeo|oêo|oi|au|èo|éo|ia|eo|ua|ii|uu|[aeiouèêé¿½]|å'
    var CAKRA  = '[yr]'
    var LKJ = '^'
    LKJ += '('+KONS+')?'             // group(1), K
    LKJ += '('+CAKRA+')?'              // group(2), C
    LKJ += '('+VOK+')'               // group(3), V
    LKJ += '('+KONS+')?'              // group(4), K
    LKJ += '('+VOK+'|'+CAKRA+')?'       // group(5), V|C
    KONSONAN = '^('+KONS+')'
    var DIGIT = '^([\u200c()\u007B\u007C\u007D,.0-9\u0020 ]+)'
    
    while (idx < iLength) {
        suku = ''
        r = iStr.match(LKJ)
        if (r != null) {
            // cari pola:
            if (r[1] != undefined) {
                if (r[4] != undefined) {
                    if (r[2] != undefined) {
                        if (r[5] != undefined) {
                            polasuku = PAT_KCV 
                        } else {
                            polasuku = PAT_KCVK 
                        }
                    } else {
                        if (r[5] != undefined) {
                            polasuku = PAT_KV
                        } else {
                            polasuku = PAT_KVK
                        }
                    }
                } else {
                    if (r[2] != undefined) {
                        polasuku = PAT_KCV
                    } else {
                        polasuku = PAT_KV
                    }
                }
            } else {
                if (r[4] != undefined) {
                    if (r[5] != undefined) {
                        polasuku = PAT_V
                    } else {
                        polasuku = PAT_VK
                    }
                } else {
                    polasuku = PAT_V
                }
            }
            
            // bentuk:
            if (polasuku == PAT_KCVK) {
                suku = r[1] + r[2] + r[3] + r[4]
                lkj  = JAWA[r[1]]
                lkj += JAWA['+' + r[2] + 'a']
                lkj += JAWA[r[3]]
                lkj += jawaahir(r[4])
            } else if (polasuku == PAT_KCV) {
                suku = r[1] + r[2] + r[3]
                lkj  = JAWA[r[1]]
                lkj += JAWA['+' + r[2] + 'a']
                lkj += JAWA[r[3]]
            } else if (polasuku == PAT_KVK) {
                suku = r[1] + r[3] + r[4]
                lkj  = JAWA[r[1]]
                lkj += JAWA[r[3]]
                lkj += jawaahir(r[4])
            } else if (polasuku == PAT_KV) {
                suku = r[1] + r[3]
                lkj  = JAWA[r[1]]
                lkj += JAWA[r[3]]
            } else if (polasuku == PAT_VK) {
                suku = r[3] + r[4]
                lkj  = JAWA[r[3].toUpperCase()]
                lkj += jawaahir(r[4])
            } else {
                suku = r[3]
                lkj = JAWA[suku.toUpperCase()]
            } // end if
            oStr += lkj //+ ':'
            tStr += suku + '(' + polasuku + '):'
        } else {
            r = iStr.match(KONSONAN)
            if (r != null) {
                suku   = r[1]
                lkj = jawaahir(suku)
                oStr += lkj
                tStr += suku + ";"
            } else {
                r = iStr.match(DIGIT)
                if (r != null) {
                    lkj = ''
                    suku = r[1]
                    l = suku.length
                    i = 0
                    while (i<l) {
                        lkj += JAWA[suku.substr(i,1)]
                        i += 1
                    } //end while
                    lkj += ''
                    oStr += lkj
                } else {
                    suku = iStr.substr(0,1)
                    lkj = suku
                    oStr += suku
                }
                //end if
                tStr += suku + '(?)'
            } // end if
        }// end if
        iStr = iStr.substr(suku.length)
        idx += suku.length
    
    }// end while
    //return tStr + '&&' + oStr
    return oStr
    //return tStr
}
////////////////////////////////////////////////////////////////////////////
var locked = false
////////////////////////////////////////////////////////////////////////////
function btJawa2Latin()
////////////////////////////////////////////////////////////////////////////
{
	if (locked == false)
	{
	
		locked = true
		
	    var jawaText = document.getElementById('JawaText').value
	
	    var latinText = Jawa2Latin(jawaText);
	    
	    document.getElementById('LatinText').value = latinText;
	
	    locked = false
	}
}
////////////////////////////////////////////////////////////////////////////
function btLatin2Jawa()
////////////////////////////////////////////////////////////////////////////
{
	if (locked == false)
	{
	
		locked = true
		
	    var latinText = document.getElementById('LatinText').value
	    
	    var jawaText = Latin2Jawa(latinText);	    
	    
	    document.getElementById('JawaText').value = jawaText;
	    
	    locked = false
	}
}
////////////////////////////////////////////////////////////////////////////
function setbg(id,color)
////////////////////////////////////////////////////////////////////////////
{
    document.getElementById(id).style.background=color
}
